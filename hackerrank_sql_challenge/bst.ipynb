    "select N,\n",
    "    case when P is null then 'Root'\n",
    "        when N not in (select distinct P from bst where P is not null) then 'Leaf'\n",
    "        else 'Inner'\n",
    "    end output\n",
    "from bst\n",
    "order by 1;"
   ]
